<http://eclipse.org/epsilon/examples/lensing> rdf:type owl:Ontology .

#################################################################
#    Data properties
#################################################################

###  http://eclipse.org/epsilon/rdf/books#publisher
books:publisher rdf:type owl:DatatypeProperty .

###  http://eclipse.org/epsilon/rdf/books#title
books:title rdf:type owl:DatatypeProperty ;
            owl:equivalentProperty fiction:title .

###  http://eclipse.org/epsilon/rdf/fiction#genres
fiction:genres rdf:type owl:DatatypeProperty .

###  http://eclipse.org/epsilon/rdf/fiction#title
fiction:title rdf:type owl:DatatypeProperty .

#################################################################
#    Classes
#################################################################

###  http://eclipse.org/epsilon/rdf/books#Book
books:Book rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                        owl:onProperty books:publisher ;
                                                        owl:someValuesFrom xsd:string
                                                      ]
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty books:title ;
                                                        owl:someValuesFrom xsd:string
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] .


###  http://eclipse.org/epsilon/rdf/fiction#FictionWork
fiction:FictionWork rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty fiction:title ;
                                          owl:someValuesFrom xsd:string
                                        ] .

