@namespace(uri="http://eclipse.org/epsilon/rdf/containment", prefix="")
package containment;

class Model {
  attr String name;
  val Entity[*] entities;
}

class Entity {
  attr String name;
  
  attr boolean eBoolean;
  attr Boolean eBooleanObject;
  attr byte eByte;
  attr Byte eByteObject;
  attr char eChar;
  attr Character eCharacterObject;
  attr double eDouble;
  attr Double eDoubleObject;
  attr float eFloat;
  attr Float eFloatObject;
  attr int eInt;
  attr Integer eIntegerObject;
  attr long eLong;
  attr Long eLongObject;
  attr short eShort;
  attr Short eShortObject;
  attr Date eDate;

  // TODO need to set multi-valued versions
  attr String[*] names;
  attr boolean[*] booleans;

  // Do not test
  // attr Object eJavaObject;
  // attr Class eJavaClass;
  // attr EObject eObject;
  // attr EClass eClass; 
}