import "../TestOperations.eol";

for (p in Project.all) {

	"\n-Start-\n".println();
	var task = p.tasks.select(i|i.title == "Implementation").first();
	task.title.println();
	var efforts = task.effort;
	reportTask(task).println("\n-------\n");
	
	var removeEffort = task.effort.first();
	task.effort.remove(removeEffort);
	reportTask(task);
	
	var analysisTask = p.tasks.select(i|i.title == "Analysis").first();
	"\n--original--\n".println();
	reportTask(analysisTask);
	(analysisTask.effort).add(removeEffort);
	"\n--Added--\n".println();
	reportTask(analysisTask);
	
	"\n-End-\n".println();
}
